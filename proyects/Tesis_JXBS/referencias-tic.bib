@article{Parker2007,
author={Parker, Jonathan },
title={Euler Equations},
journal={The New Palgrave Dictionary of Economics},
year={2007},
publisher={Palgrave Macmillan UK},
address={London},
edition= {2},
pages={1--6},
abstract={An Euler equation is a difference or differential equation that is an intertemporal first-order condition for a dynamic choice problem. It describes the evolution of economic variables along an optimal path. It is a necessary but not sufficient condition for a candidate optimal path, and so is useful for partially characterizing the theoretical implications of a range of models for dynamic behaviour. In models with uncertainty, expectational Euler equations are conditions on moments, and thus directly provide a basis for testing models and estimating model parameters using observed dynamic behaviour.},
}



